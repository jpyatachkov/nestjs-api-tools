{
  "name": "nestjs-api-tools",
  "version": "1.0.0",
  "description": "Wrappers for convenient API development including validation, logging and i18n wrappers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12.x || 14.x"
  },
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpyatachkov/nestjs-api-tools.git"
  },
  "keywords": [
    "nestjs",
    "cache",
    "discord",
    "pipe",
    "validation",
    "guard"
  ],
  "author": "jpyatachkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpyatachkov/nestjs-api-tools/issues"
  },
  "homepage": "https://github.com/jpyatachkov/nestjs-api-tools#readme",
  "dependencies": {
    "@nestjs/common": "^8.2.1",
    "@nestjs/core": "^8.2.1",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "discord.js": "^12.5.3",
    "ip-range-check": "^0.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "nestjs-i18n": "^8.1.9",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "typeorm": "^0.2.40"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@nestjs/testing": "^8.2.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/luxon": "^2.0.7",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": true,
        "diagnostics": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "./jest-setup.ts"
    ]
  }
}
